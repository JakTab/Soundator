{
  "name": "soundator",
  "productName": "Soundator",
  "version": "0.1.0",
  "description": "Soundator is a minimalist-esque audio player",
  "author": "JakTab",
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "postpackage": "electron-packager ./ --out=./release-builds --overwrite --icon=./assets/icon.ico",
    "packageBuild": "npm run build && electron-builder"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "album-art": "^2.0.2",
    "byte-base64": "^1.1.0",
    "electron-store": "^6.0.1",
    "interactjs": "^1.10.0",
    "mousetrap": "^1.6.5",
    "musicmetadata": "^2.0.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-tooltip": "^4.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "babili-webpack-plugin": "^0.1.2",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "electron": "^9.3.2",
    "electron-builder": "^22.8.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^15.1.0",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.3",
    "style-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "build": {
    "productName": "Soundator",
    "appId": "jaktab.Soundator",
    "win": {
      "target": "portable",
      "icon": "/assets/icon.ico"
    },
    "portable": {
      "artifactName": "Soundator_Portable_0.1.0.exe"
    },
    "directories": {
      "output": "release-builds",
      "buildResources": "release-builds/buildResources"
    }
  }
}
